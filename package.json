{
  "name": "napicu-web2",
  "version": "2.0.0",
  "author": "Numax",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-openapi-dev": "openapi-generator-cli generate -o ./OpenAPI -i http://localhost:8080/v3/api-docs/ -g typescript-angular",
    "build-openapi-prod": "openapi-generator-cli generate -o ./OpenAPI -i https://api.napicu.eu/v3/api-docs/ -g typescript-angular",
    "build": "ng build  --build-optimizer --extract-licenses --output-hashing none",
    "buildall": "node build.js",
    "watch": "ng build --watch --configuration development",
    "pwa": "ng add @angular/pwa --project"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.0.6",
    "@angular/cdk": "^14.0.5",
    "@angular/common": "~14.0.6",
    "@angular/compiler": "~14.0.6",
    "@angular/core": "~14.0.6",
    "@angular/forms": "~14.0.6",
    "@angular/material": "^14.0.5",
    "@angular/platform-browser": "~14.0.6",
    "@angular/platform-browser-dynamic": "~14.0.6",
    "@angular/router": "~14.0.6",
    "@angular/service-worker": "~14.0.6",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "particles.js": "^2.0.0",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.0.6",
    "@angular/cli": "~14.0.6",
    "@angular/compiler-cli": "~14.0.6",
    "@types/jasmine": "~4.0.3",
    "@types/node": "^18.0.6",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "typescript": "~4.7.4"
  }
}
