{
  "name": "napicu-web2",
  "version": "2.0.0",
  "author": "Numax",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-openapi-dev": "openapi-generator-cli generate -o ./OpenAPI -i http://localhost:8080/v3/api-docs/ -g typescript-angular",
    "build-openapi-prod": "openapi-generator-cli generate -o ./OpenAPI -i https://api.napicu.eu/v3/api-docs/ -g typescript-angular",
    "build": "ng build  --build-optimizer --extract-licenses --output-hashing none",
    "buildall": "node build.js",
    "watch": "ng build --watch --configuration development",
    "pwa": "ng add @angular/pwa --project"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.2.7",
    "@angular/cdk": "^15.2.7",
    "@angular/common": "~15.2.7",
    "@angular/compiler": "~15.2.7",
    "@angular/core": "~15.2.7",
    "@angular/forms": "~15.2.7",
    "@angular/material": "^15.2.7",
    "@angular/platform-browser": "~15.2.7",
    "@angular/platform-browser-dynamic": "~15.2.7",
    "@angular/router": "~15.2.7",
    "@angular/service-worker": "~15.2.7",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@techiediaries/ngx-qrcode": "^9.1.0",
    "particles.js": "^2.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.6",
    "@angular/cli": "~15.2.6",
    "@angular/compiler-cli": "~15.2.7",
    "@types/jasmine": "~4.3.1",
    "@types/node": "^18.15.11",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "typescript": "4.9.5"
  }
}
